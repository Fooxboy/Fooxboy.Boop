<Application x:Class="Fooxboy.Boop.Client.WpfApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Fooxboy.Boop.Client.WpfApp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="GoogleSansBold">./Resources/Fonts/GoogleSans-Bold.ttf#Google Sans Bold</FontFamily>
            <FontFamily x:Key="GoogleSansRegular">./Resources/Fonts/GoogleSans-Regular.ttf#Google Sans Regular</FontFamily>
            <FontFamily x:Key="GoogleSansMedium">./Resources/Fonts/GoogleSans-Medium.ttf#Google Sans Medium</FontFamily>

            <ResourceDictionary.MergedDictionaries>
                <!--  FluentWPF Controls  -->
                <ResourceDictionary Source="pack://application:,,,/FluentWPF;component/Styles/Controls.xaml" />
                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
